
{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**",
                    "server/**",
                    "src/**",
                    "e2e/**",
                    "package.json",
                    "package-lock.json"
                ],
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
   
}
